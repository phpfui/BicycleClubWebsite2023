{
    "name": "phpfui/bicycle_club_website",
    "description": "A mobile friendly website to manage a bicycle club",
    "minimum-stability": "stable",
    "keywords": ["bicycle", "club", "website"],
    "license": "AGPL-3.0-or-later",
    "authors": [
        {
        "name": "Bruce Wells",
        "email": "brucekwells@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "App\\": "App\\"
        }
    },
    "require": {
        "php": "=8.4",
        "alek13/slack": "^2.0",
        "endroid/qr-code": "^6.0",
        "flowjs/flow-php-server": "^2.0",
        "gitonomy/gitlib": "^1.0",
        "guzzlehttp/guzzle": "^7.9",
        "ifsnop/mysqldump-php": "^2.8",
        "intervention/image": "^3.0",
        "kwn/number-to-words": "^2.8",
        "laminas/laminas-uri": "^2.9",
        "mpdf/mpdf": "^8.1",
        "nxp/math-executor": "^2.3",
        "php-debugbar/php-debugbar": "^v2",
        "php-http/discovery": "^1.19",
        "php-http/guzzle7-adapter": "^1.0",
        "php-http/message": "^1.16",
        "phpfui/common": "dev-main",
        "phpfui/constantcontact": "dev-main",
        "phpfui/icalendar": "^1.0",
        "phpfui/php-imap": "dev-main",
        "phpfui/instadoc": "^1.0",
        "phpfui/orm": "^2.0",
        "phpfui/phpfui": "^6.2",
        "phpfui/recaptcha": "^2.0",
        "phpfui/translation": "^1.0",
        "phpmailer/phpmailer": ">=6.0",
        "psr-php7/log": "^1.0",
        "psr/http-factory": "^1.0",
        "psr/http-message": "^2.0",
        "scrivo/highlight.php": "^9.0",
        "setasign/fpdf": "^1.8",
        "tinify/tinify": "*",
        "tinymce/tinymce": "^7.0",
        "twilio/sdk": "^8.0",
        "zbateson/mail-mime-parser": "^3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^12.0",
        "phpfui/html-unit-tester": "^1.0",
        "roave/security-advisories": "dev-latest",
        "phpfui/phpunit-syntax-coverage": "^1.0",
        "friendsofphp/php-cs-fixer": ">=3.0",
        "phpstan/phpstan": "^2.0"
    },
    "config": {
        "allow-plugins": {
            "php-http/discovery": true
        }
    },
    "repositories": [
        {
        "type": "vcs",
        "url": "https://github.com/phpfui/common"
        }
    ]
}
